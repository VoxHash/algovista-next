# AlgoVista Environment Variables
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# ============================================
# Site Configuration
# ============================================
# NEXT_PUBLIC_SITE_URL=https://algovista.voxhash.dev
# NEXT_PUBLIC_SITE_NAME=AlgoVista

# ============================================
# Analytics (Optional)
# ============================================
# Uncomment and add your analytics IDs
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_GTM_ID=GTM-XXXXXXX

# ============================================
# Search Engine Verification (Optional)
# ============================================
# Add your verification codes from Google Search Console, Bing Webmaster Tools, etc.
# GOOGLE_VERIFICATION_CODE=your-google-verification-code
# BING_VERIFICATION_CODE=your-bing-verification-code

# ============================================
# Social Media (Optional)
# ============================================
# NEXT_PUBLIC_TWITTER_HANDLE=@VoxHash
# NEXT_PUBLIC_GITHUB_URL=https://github.com/VoxHash/algovista-next
# NEXT_PUBLIC_YOUTUBE_URL=https://youtube.com/@VoxHash

# ============================================
# Feature Flags (Optional)
# ============================================
# Enable or disable features
# NEXT_PUBLIC_ENABLE_ANALYTICS=true
# NEXT_PUBLIC_ENABLE_PWA=true
# NEXT_PUBLIC_ENABLE_DARK_MODE=true

# ============================================
# API Keys (Optional - for future features)
# ============================================
# NEXT_PUBLIC_API_KEY=your-api-key
# API_SECRET_KEY=your-secret-key

# ============================================
# Development Settings (Optional)
# ============================================
# NODE_ENV=development
# NEXT_PUBLIC_DEBUG=false
